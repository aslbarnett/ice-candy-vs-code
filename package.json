{
  "name": "ice-candy-vscode",
  "displayName": "Ice Candy",
  "description": "A theme for Visual Studio Code.",
  "version": "1.0.0",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Ice Candy",
        "uiTheme": "vs-dark",
        "path": "./themes/ice-candy-color-theme.json"
      }
    ]
  },
  "scripts": {
    "build": "ts-node ./src/index.ts",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^18.7.15",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "vsce": "^2.15.0"
  }
}
